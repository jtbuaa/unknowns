<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_height="fill_parent"
	android:layout_width="@dimen/layout_width">

	<EditText android:layout_height="wrap_content"
		android:cursorVisible="false" android:text="0." android:layout_width="fill_parent"
		android:gravity="right" android:id="@+id/EditTextDisplayBar"></EditText>





	<LinearLayout android:id="@+id/LinearLayout01"
		android:layout_height="wrap_content" android:layout_width="fill_parent">
		<TextView android:layout_height="wrap_content"
			android:background="@color/gray" android:id="@+id/TextViewM"
			android:layout_width="wrap_content" android:height="30px"
			android:width="30px" android:layout_marginLeft="5px" android:gravity="center"></TextView>
		<Button android:layout_height="wrap_content"
			android:layout_weight="1" android:id="@+id/ButtonBackspace"
			android:text="Backspace" android:layout_width="fill_parent"></Button>
		<Button android:layout_height="wrap_content" android:id="@+id/ButtonCE"
			android:text="CE" android:layout_weight="1" android:layout_width="fill_parent"></Button>
		<Button android:layout_height="wrap_content" android:id="@+id/ButtonC"
			android:text="C" android:layout_weight="1" android:layout_width="fill_parent"></Button>
	</LinearLayout>



	<LinearLayout android:id="@+id/LinearLayout02"
		android:layout_height="wrap_content"
		android:orientation="horizontal" android:layout_width="fill_parent">
		<LinearLayout android:id="@+id/LinearLayout03"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:orientation="vertical">
			<Button android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:id="@+id/ButtonMC"
				android:text="MC"></Button>
			<Button android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:text="MR" android:id="@+id/ButtonMR"></Button>
			<Button android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:id="@+id/ButtonMS"
				android:text="MS"></Button>
			<Button android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:text="M+" android:id="@+id/ButtonMP"></Button>
		</LinearLayout>
		<TableLayout android:id="@+id/TableLayout01"
			android:layout_height="wrap_content" android:layout_width="fill_parent">
			<TableRow android:id="@+id/TableRow01" android:layout_height="wrap_content" android:layout_width="fill_parent">
				<Button android:text="7" android:id="@+id/Button7"
					android:layout_height="wrap_content" android:layout_weight="1" android:layout_width="fill_parent"></Button>
				<Button android:text="8" android:id="@+id/Button8"
					android:layout_height="wrap_content" android:layout_weight="1" android:layout_width="fill_parent"></Button>
				<Button android:text="9" android:id="@+id/Button9"
					android:layout_height="wrap_content" android:layout_weight="1" android:layout_width="fill_parent"></Button>
				<Button android:text="/" android:id="@+id/ButtonDivision"
					android:layout_height="wrap_content" android:layout_weight="1" android:layout_width="fill_parent"></Button>
				<Button android:text="sqrt" android:id="@+id/ButtonSqrt"
					android:layout_height="wrap_content" android:layout_weight="1" android:layout_width="fill_parent"></Button>
			</TableRow>
			<TableRow android:id="@+id/TableRow02" android:layout_width="fill_parent"
				android:layout_height="wrap_content">
				<Button android:text="4" android:id="@+id/Button4"
					android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_weight="1"></Button>
				<Button android:text="5" android:id="@+id/Button5"
					android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_weight="1"></Button>
				<Button android:text="6" android:id="@+id/Button6"
					android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_weight="1"></Button>
				<Button android:text="*" android:id="@+id/ButtonMultiplication"
					android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_weight="1"></Button>
				<Button android:text="%" android:id="@+id/ButtonPercent"
					android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_weight="1"></Button>
			</TableRow>
			<TableRow android:id="@+id/TableRow03" android:layout_width="fill_parent"
				android:layout_height="wrap_content">
				<Button android:text="1" android:id="@+id/Button1"
					android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_weight="1"></Button>
				<Button android:text="2" android:id="@+id/Button2"
					android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_weight="1"></Button>
				<Button android:text="3" android:id="@+id/Button3"
					android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_weight="1"></Button>
				<Button android:text="-" android:id="@+id/ButtonMinus"
					android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_weight="1"></Button>
				<Button android:text="1/x" android:id="@+id/ButtonInverse"
					android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_weight="1"></Button>
			</TableRow>
			<TableRow android:id="@+id/TableRow04" android:layout_width="fill_parent"
				android:layout_height="wrap_content">
				<Button android:text="0" android:id="@+id/Button0"
					android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_weight="1"></Button>
				<Button android:text="+/-" android:id="@+id/ButtonNegative"
					android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_weight="1"></Button>
				<Button android:text="." android:id="@+id/ButtonDecimal"
					android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_weight="1"></Button>
				<Button android:text="+" android:id="@+id/ButtonPlus"
					android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_weight="1"></Button>
				<Button android:text="=" android:id="@+id/ButtonEqual"
					android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_weight="1"></Button>
			</TableRow>
		</TableLayout>
	</LinearLayout>




</LinearLayout>
